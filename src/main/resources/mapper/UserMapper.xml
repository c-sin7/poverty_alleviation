<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.itsource.poverty_alleviation.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.itsource.poverty_alleviation.domain.User">
        <id column="ID" property="id" />
        <result column="uname" property="uname" />
        <result column="password" property="password" />
        <result column="email" property="email" />
        <result column="registration_time" property="registrationTime" />
        <result column="last_login" property="lastLogin" />
        <result column="sculpture" property="sculpture" />
        <result column="introduction" property="introduction" />
        <result column="privilege" property="privilege" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, uname, password, email, registration_time, last_login, sculpture, introduction, privilege
    </sql>

</mapper>
